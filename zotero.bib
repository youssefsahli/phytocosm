@article{jakovljevic_kovac_determination_2021,
	title = {Determination of Suitable Macroporous Resins and Desorbents for Carnosol and Carnosic Acid from Deep Eutectic Solvent Sage (Salvia officinalis) Extract with Assessment of Antiradical and Antibacterial Activity},
