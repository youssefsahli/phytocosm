@incollection{xiaoying_glycyrrhiza_2017,
	title = {\textit{Glycyrrhiza glabra} (Licorice)},
	isbn = {978-0-12-805413-0},
	url = {https://www.sciencedirect.com/science/article/pii/B9780128054130000144},
	abstract = {The licorice (Radix Glycyrrhizae or Liquiritiae radix), which is broadly used in medicine and commerce, is derived from the sweet root of various species of Glycyrrhiza (Glycyrrhiza uralensis Fisch., Glycyrrhiza glabra L., or Glycyrrhiza inflata Bat., Leguminosae). Licorice is native to southern Europe and parts of Asia; it is one of the most commonly used herbal medicines and an important source of confectionery (Fiore et al., 2005). Therefore the health properties associated with licorice are well documented. According to the World Health Organization, licorice is employed as a demulcent in the treatment of sore throats and an expectorant for coughs and bronchial catarrh. Licorice also has a critical role in the prophylaxis and treatment of gastric and duodenal ulcers as well as dyspepsia. As an antiinflammatory agent, licorice reduces allergic reactions and prevents liver toxicity. In China, licorice is also documented to be effective for fatigue and debilitation (China, 2015). In this chapter, active components, extractions or preparations of liquorice in the medical use included protection of the nervous, endocrine, respiratory, digestive and cardiovascular systems that are supported by experimental or clinical data will be summarized.},
	pages = {231--250},
	booktitle = {Sustained Energy for Enhanced Human Functions and Activity},
	publisher = {Academic Press},
	author = {Xiaoying, Wang and Han, Zhang and Yu, Wang},
	editor = {Bagchi, Debasis},
	urldate = {2025-04-20},
	date = {2017-01-01},
	doi = {10.1016/B978-0-12-805413-0.00014-4},
	keywords = {Airway protection, Ethnobotany, Glycyrrhizin, Hepatoprotective effect, Immunity regulation, Licorice},
}

