	number = {2},
	journaltitle = {Journal of the American Academy of Dermatology},
	shortjournal = {J Am Acad Dermatol},
	author = {Bernstein, E. F. and Chen, Y. Q. and Kopp, J. B. and Fisher, L. and Brown, D. B. and Hahn, P. J. and Robey, F. A. and Lakkakorpi, J. and Uitto, J.},
