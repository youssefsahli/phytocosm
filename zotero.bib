	author = {Li, Chia-Yang and Suzuki, Katsuhiko and Hung, Yung-Li and Yang, Meng-Syuan and Yu, Chung-Ping and Lin, Shiuan-Pey and Hou, Yu-Chi and Fang, Shih-Hua},
