	keywords = {Blotting, Northern, Cells, Cultured, Collagen, Collagenases, Coloring Agents, Elastic Tissue, Elastin, Environmental Exposure, Gene Expression Regulation, Gene Expression Regulation, Enzymologic, Humans, Immunohistochemistry, Male, Microscopy, Confocal, Middle Aged, {RNA}, Messenger, Skin, Skin Aging, Sunlight},
