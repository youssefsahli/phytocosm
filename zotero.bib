	number = {3},
	journaltitle = {Photochemistry and Photobiology},
	shortjournal = {Photochem Photobiol},
	author = {Mo, Xiaoying and Chen, Xiaochun and Pan, Xiaojiao and Lu, Yantong and Pan, Guangjuan and Xie, Jielan and Pan, Zhenzhen and Li, Li and Tian, Hui and Li, Yaohua},
	date = {2024},
