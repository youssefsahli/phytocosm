	date = {2019-06},
	pmid = {30964240},
	keywords = {Air Pollution, Cosmetics, Environmental Exposure, Face, Humans, Hyaluronic Acid, Skin, Skin Absorption, Skin Aging, Sunlight, Water, exposome, mineral 89, mineralizing thermal water, skin barrier function},
}

