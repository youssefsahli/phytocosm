	pmid = {32183224},
	pmcid = {PMC7144722},
	keywords = {Aloe, Aloe vera, Anti-Infective Agents, Anti-Inflammatory Agents, Antineoplastic Agents, Cardiotonic Agents, Humans, Phytochemicals, extracts, isolated compounds, pharmacology},
}

