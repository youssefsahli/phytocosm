@article{taieb_deciphering_2024,
	title = {Deciphering the Role of Androgen in the Dermatologic Manifestations of Polycystic Ovary Syndrome Patients: A State-of-the-Art Review},
	volume = {14},
	rights = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2075-4418},
