	number = {40},
	journaltitle = {Current Pharmaceutical Design},
	author = {Hoseinsalari, Afsaneh and Atapour-Mashhad, Hoda and Asili, Javad and Tayarani-Najaran, Zahra and Mohtashami, Leila and Khanavi, Mahnaz and Vazirian, Mahdi and Javar, Hamid Akbari and Niknam, Somayeh and Golmohammadzadeh, Shiva and Ardekani, Mohammad Reza},
	urldate = {2025-05-14},
	date = {2024-01-01},
	langid = {english},
	note = {Number: 40},
	keywords = {Glycyrrhiza glabra, {NLC}, anti-tyrosinase., glabridin, hyperpigmentation, liquiritin},
}

