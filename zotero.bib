@article{capuzzo_antioxidant_2014,
	title = {Antioxidant and radical scavenging activities of chamazulene},
	volume = {28},
	issn = {1478-6427},
	doi = {10.1080/14786419.2014.931393},
	abstract = {Essential oils ({EOs}) of chamomile contain several bioactive compounds, including monoterpenes, sesquiterpenes, triterpenes and fatty acids. Hydrodistillation of chamomile {EO} induces the formation of chamazulene, a bioactive compound. Chamazulene was isolated from the {EO} by column chromatography. The total antioxidant capacity confirmed a higher antioxidant activity of chamazulene ({IC}50 = 6.4 μg {mL}(- 1)) than of ascorbic acid ({IC}50 = 12.8 μg {mL}(- 1)), α-tocopherol ({IC}50 = 20.5 μg {mL}(- 1)) and of butylated hydroxytoluene ({BHT}) ({IC}50 = 30.8 μg {mL}(- 1)). Chamazulene was unable to react with {DPPH}√. However, when chamazulene was assayed with {ABTS}√, a strong and significantly (P {\textless} 0.05) higher free radical scavenging activity was observed ({IC}50 = 3.7 μg {mL}(- 1)), with respect to {BHT} ({IC}50 = 6.2 μg {mL}(- 1)) and α-tocopherol ({IC}50 = 11.5 μg {mL}(- 1)). The results of this work show that chamazulene is an important factor for the antioxidant power of chamomile oil.},
	pages = {2321--2323},
	number = {24},
	journaltitle = {Natural Product Research},
	shortjournal = {Nat Prod Res},
	author = {Capuzzo, Andrea and Occhipinti, Andrea and Maffei, Massimo E.},
