	abstract = {As the cosmetics industry adapts to meet the growing consumer demand for sustainable products, this study investigates how these preferences translate into consumer valuation. It focuses on the influence of product attributes and individual values on consumer perceptions of price in the realm of sustainable cosmetics. This study examines how product attributes and personal variables affect price perception within the sustainable cosmetics industry. Utilizing conjoint analysis, it identifies two distinct consumer segments: ‘Eco-Conscious Explorers' and ‘Pragmatic Wellness Advocates'. The first group prioritizes products with eco-friendly packaging and sustainability certifications, while the second values health benefits and sensory experience. Using conjoint analysis, the study reveals that personal factors like Health Consciousness and Environmental Concerns greatly influence purchase decisions within these segments. The findings indicate a complex decision-making environment, with each group displaying distinct preferences that are critical for the sustainable cosmetics market. This research provides actionable insights for developing targeted marketing strategies that align with the diverse preferences of these consumer groups, potentially driving more sustainable buying behaviors.},
	pages = {144730},
	journaltitle = {Journal of Cleaner Production},
	shortjournal = {Journal of Cleaner Production},
	author = {Szaban, Marta and Szymkowiak, Andrzej and Zdziennicka, Anna},
	urldate = {2025-05-14},
	date = {2025-01-20},
	keywords = {Consumer behavior, Personal values, Price perception, Product attributes, Sustainable consumption, Sustainable labeling},
}

